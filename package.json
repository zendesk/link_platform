  "repository": {
    "type": "git",
    "url": "git+https://github.com/zendesk/link_platform.git"
  },
  "homepage": "https://github.com/zendesk/link_platform#readme",
  "bugs": {
    "url": "https://github.com/zendesk/link_platform/issues"
  },
  "proxy": {
    "/api/*":{
      "target": "http://blomp.localhost:3000"
    }
  }
